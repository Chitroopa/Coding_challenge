class Solution:
    def addStrings(self, num1: str, num2: str) -> str:
        
        def convert_to_int(nums):
            result = 0
            for i in range(len(nums)):
                result += int(nums[i]) * 10 **(len(nums)-1-i)
            return result
        
        return str(convert_to_int(num1) + convert_to_int(num2))
        
        
